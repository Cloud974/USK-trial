{
  "name": "react-universally",
  "version": "13.0.0",
  "description": "A starter kit giving you the minimum requirements for a modern universal react application.",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=7.5 <8"
  },
  "scripts": {
    "analyze": "cross-env NODE_ENV=production babel-node internal/scripts/analyze",
    "build:dev": "cross-env NODE_ENV=development babel-node internal/scripts/build",
    "build": "rm -rf ./build && cross-env NODE_ENV=production babel-node internal/scripts/build",
    "clean": "babel-node internal/scripts/clean",
    "deploy": "babel-node internal/scripts/deploy",
    "development": "babel-node internal/development",
    "dev": "babel-node internal/development",
    "eslint": "eslint src config tools",
    "lint": "npm run eslint; npm run stylelint",
    "preinstall": "node internal/scripts/preinstall",
    "postinstall": "if [ \"x$NODE_ENV\" != \"x\" ] && [ \"$NODE_ENV\" != \"development\" ]; then npm run build; fi;",
    "start": "node build/server",
    "stylelint": "stylelint \"shared/**/*.scss\" --syntax scss",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "contributors": [
    "Alin Porumb",
    "Benjamin Kniffler <bkniffler@me.com>",
    "Carson Perrotti <carsonperrotti@gmail.com>",
    "Christian Glombek <christian.glombek@rwth-aachen.de>",
    "Christoph Werner",
    "David Edmondson",
    "Dion Dirza",
    "Evgeny Boxer",
    "Joe Kohlmann <kohlmannj@mac.com>",
    "Lucian Lature <lucian.lature@gmail.com>",
    "Steven Enten <steven@enten.fr>",
    "Sean Matheson <sean@ctrlplusb.com>",
    "Steven Truesdell <steven@strues.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|internal|node_modules|flow-typed|public)/"
    ]
  },
  "dependencies": {
    "app-root-dir": "1.0.2",
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.22.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-template": "6.22.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chokidar": "1.6.1",
    "classnames": "^2.2.5",
    "classnames-loader": "^2.0.0",
    "colors": "1.1.2",
    "component-webpack-resolver-plugin": "^2.0.2",
    "compression": "1.6.2",
    "core-decorators": "^0.15.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "0.9.0",
    "glob": "7.1.1",
    "happypack": "3.0.2",
    "helmet": "3.4.0",
    "hpp": "0.2.1",
    "html-webpack-plugin": "2.26.0",
    "isomorphic-fetch": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "md5": "2.2.1",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "mobx-utils": "^2.0.1",
    "modernizr": "3.3.1",
    "modernizr-loader": "1.0.1",
    "node-notifier": "4.6.1",
    "node-sass": "^4.5.0",
    "normalize.css": "5.0.0",
    "offline-plugin": "4.5.5",
    "postcss": "^5.2.12",
    "postcss-csso": "^1.1.2",
    "postcss-loader": "^1.2.2",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-async-component": "0.2.1",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "4.0.0-beta.5",
    "react-router-dom": "4.0.0-beta.5",
    "react-svgdom-loader": "^3.0.0",
    "regenerator-runtime": "0.10.1",
    "rimraf": "2.5.4",
    "sass-loader": "^5.0.1",
    "semver": "5.3.0",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.10",
    "style-loader": "0.13.1",
    "user-home": "2.0.0",
    "uuid": "3.0.1",
    "webpack": "2.2.0",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "enzyme": "2.7.1",
    "enzyme-to-json": "1.4.5",
    "eslint": "3.14.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "jest": "18.1.0",
    "stylelint": "^7.8.0",
    "stylelint-config-primer": "^1.4.0"
  }
}
